{
  "variables": {},
  "title": "Laravel Frontend Development",
  "description": "Beginners guide for frontend development in Laravel",
  "authors": ["Dhia Eddine Nabli"],
  "copyright": "Dhia Eddine Nabli",
  "keywords": ["Laravel","Livewire","Vue.js"],
  "language": "en",
  "hidepageheaders": [],
  "hidepagefooters": [],
  "plugins": [
    "chapter-fold",
    "hide-published-with",
    "hints",
    "mermaid-newface"
  ],
  "pdf": {
    "pageNumbers": true,
    "margin": {
      "right": 32,
      "left": 32
    }
  },
  "pluginsConfig": {
    "asciidoc": {
      "numbered": {},
      "chapter-fold":{},
      "mermaid-newface": {
        "theme": "neutral"
      },
      "hints": {
        "info": "fa fa-info-circle",
        "tip": "fa fa-mortar-board",
        "danger": "fa fa-exclamation-cicle",
        "working": "fa fa-wrench"
      }
    }
  }
}